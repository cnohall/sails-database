<h1 class="disiplay-4">Materials</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Description</th>
            <th>Time Factor</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% materials.forEach(function(material){ %>
            <tr>
                <td><%= material.id %></td>
                <td><%= material.name %></td>
                <td><%= material.description %></td>
                <td><%= material.timeFactor %></td>
                <td>
                    <a href="/materials/edit/<%= material.id %>">
                        <input type="submit" value="Edit" class="btn btn-primary">
                    </a>
                    <a href="/materials/destroy/<%= material.id %>">
                        <input type="submit" value="Delete" class="btn btn-danger">
                    </a>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>